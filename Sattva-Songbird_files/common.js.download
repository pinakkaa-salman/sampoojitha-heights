            dataType: "text",
            beforeSend: function() {
                // $('.call_now_button').prop('disabled', false);
                // $('.alert_inside_call_now').css('display', 'block');
            },
            success: function(result) {
                // $('#click_to_call_Modal').modal('hide');
            },
