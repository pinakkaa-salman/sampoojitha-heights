$(document).on('click', '.compare_button_left', function() {
    if ($('.fa-arrow-right').length > 0) {
        $('.fa-arrow-right').addClass('fa-arrow-left').removeClass('fa-arrow-right');
        $('.compare_div_right').show();
        $('.compare_button_left').css('margin-left', '300px');
        // $('.compare_project_text').hide();
    } else {
        $('.fa-arrow-left').addClass('fa-arrow-right').removeClass('fa-arrow-left');
        // $('.compare_project_text').show();
        $('.compare_div_right').hide();
        // $(".overlay").hide();
        $('.compare_button_left').css('margin-left', '0px');
    }
});

